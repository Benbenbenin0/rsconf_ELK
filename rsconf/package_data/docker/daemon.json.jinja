{
    "iptables": {{ docker.iptables | tojson }},
    "live-restore": true,
    "log-driver": "journald",
    {% if docker_registry.http_host is defined %}
    "registry-mirrors": ["{{ docker_registry.http_host }}"],
    {% endif %}
    "storage-driver": "overlay2",
    "storage-opts": [
        "overlay2.override_kernel_check=true"
    ]
}
