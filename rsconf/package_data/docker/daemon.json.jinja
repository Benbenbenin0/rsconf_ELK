{
    "iptables": {{ docker_iptables | tojson }},
    {# does not work with docker swarm: "live-restore": true, #}
    "log-driver": "journald",
    {% if docker_registry_http_host is defined %}
    "registry-mirrors": ["{{ docker_registry_http_host }}"],
    {% endif %}
    "storage-driver": "overlay2",
    "storage-opts": [
        "overlay2.override_kernel_check=true"
    ]
}
